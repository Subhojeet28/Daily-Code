#include <iostream>
using namespace std;


int main() {
    int n;
   int arr[n];
   cin>>n;
   //int arr[n]; 
   int start=0;
   int end=n-1;//it shows the array index
   while(start<end)//strting will be always less than end as it start from 0
   {
       int temp=arr[start];
       arr[start]=arr[end];
       arr[end]=temp;
       start=start+1;
       end=end-1;
   }
   for(int i=0;i<n;i++)//to traverse the array
   {
       cout<<arr[i]<<" ";
   }
   
   
    
    
    
    return 0;
}
